<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<modDesc descVersion="40">
    <author>TorkModding, MdkMods, The Expendables, Pope</author>
    <version>1.0.0.5</version>
    <title>
        <en>Landcruiser J70</en>
        <de>Landcruiser J70</de>
    </title>
    <description>
        <en>Landcruiser J70</en>
        <de>Landcruiser J70</de>
    </description>
    <specializations>
        <specialization name="logsWinch" className="LogsWinch" filename="script/logsWinch.lua"/>
    </specializations>
    <vehicleTypes>
        <type name="J70Winch" parent="carFillable" className="Vehicle" filename="$dataS/scripts/vehicles/Vehicle.lua">
            <specialization name="logsWinch"/>
            <specialization name="turnOnVehicle"/>
            <specialization name="powerConsumer"/>
            <specialization name="attachable"/>
        </type>
    </vehicleTypes>
    <actions>
        <action name="LOGSWINCH_ATTACH_DETACH" ignoreComboMask="false" axisType="HALF" category="ONFOOT"/>
        <action name="LOGSWINCH_DETACH_ALL" ignoreComboMask="false" axisType="HALF" category="VEHICLE"/>
        <action name="LOGSWINCH_WINCH_PLAYER" ignoreComboMask="false" axisType="HALF" category="ONFOOT"/>
        <action name="LOGSWINCH_WINCH_VEHICLE" ignoreComboMask="false" axisType="HALF" category="VEHICLE"/>
        <action name="LOGSWINCH_WINCH_SPEEDUP_PLAYER" ignoreComboMask="false" axisType="HALF" category="ONFOOT"/>
        <action name="LOGSWINCH_WINCH_SPEEDUP_VEHICLE" ignoreComboMask="false" axisType="HALF" category="VEHICLE"/>
        <action name="LOGSWINCH_RELEASE_PLAYER" ignoreComboMask="false" axisType="HALF" category="ONFOOT"/>
        <action name="LOGSWINCH_RELEASE_VEHICLE" ignoreComboMask="false" axisType="HALF" category="VEHICLE"/>
        <action name="LOGSWINCH_SWITCH_TO_NEXT" ignoreComboMask="false" axisType="HALF" category="ONFOOT"/>
        <action name="LOGSWINCH_SWITCH_TRAILER_HOOK" ignoreComboMask="false" axisType="HALF" category="ONFOOT"/>
    </actions>
    <inputBinding>
        <actionBinding action="LOGSWINCH_ATTACH_DETACH">
            <binding device="KB_MOUSE_DEFAULT" input="KEY_b" axisComponent="+" neutralInput="0" index="1"/>
        </actionBinding>
        <actionBinding action="LOGSWINCH_DETACH_ALL">
            <binding device="KB_MOUSE_DEFAULT" input="KEY_lshift KEY_b" axisComponent="+" neutralInput="0" index="1"/>
        </actionBinding>
        <actionBinding action="LOGSWINCH_WINCH_PLAYER">
            <binding device="KB_MOUSE_DEFAULT" input="KEY_z" axisComponent="+" neutralInput="0" index="1"/>
        </actionBinding>
        <actionBinding action="LOGSWINCH_WINCH_VEHICLE">
            <binding device="KB_MOUSE_DEFAULT" input="KEY_z" axisComponent="+" neutralInput="0" index="1"/>
        </actionBinding>
        <actionBinding action="LOGSWINCH_WINCH_SPEEDUP_PLAYER">
            <binding device="KB_MOUSE_DEFAULT" input="KEY_lshift KEY_z" axisComponent="+" neutralInput="0" index="1"/>
        </actionBinding>
        <actionBinding action="LOGSWINCH_WINCH_SPEEDUP_VEHICLE">
            <binding device="KB_MOUSE_DEFAULT" input="KEY_lshift KEY_z" axisComponent="+" neutralInput="0" index="1"/>
        </actionBinding>
        <actionBinding action="LOGSWINCH_RELEASE_PLAYER">
            <binding device="KB_MOUSE_DEFAULT" input="KEY_x" axisComponent="+" neutralInput="0" index="1"/>
        </actionBinding>
        <actionBinding action="LOGSWINCH_RELEASE_VEHICLE">
            <binding device="KB_MOUSE_DEFAULT" input="KEY_lshift KEY_x" axisComponent="+" neutralInput="0" index="1"/>
        </actionBinding>
        <actionBinding action="LOGSWINCH_SWITCH_TO_NEXT">
            <binding device="KB_MOUSE_DEFAULT" input="KEY_lshift KEY_g" axisComponent="+" neutralInput="0" index="1"/>
        </actionBinding>
        <actionBinding action="LOGSWINCH_SWITCH_TRAILER_HOOK">
            <binding device="KB_MOUSE_DEFAULT" input="KEY_o" axisComponent="+" neutralInput="0" index="1"/>
        </actionBinding>
    </inputBinding>
