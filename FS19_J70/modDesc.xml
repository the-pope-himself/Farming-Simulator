    <brands>
        <brand name="TOYOTA" title="Toyota" image="brand.dds"/>
    </brands>
    <iconFilename>store.dds</iconFilename>
    <storeItems>
        <storeItem rootNode="vehicle" xmlFilename="ToyotaLandcruiser.xml"/>
    </storeItems>
    <multiplayer supported="true"/>
